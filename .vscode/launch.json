{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to .NET Functions",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:azureFunctions.pickProcess}"
    },
    {
      "name": ".NET Core Console",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceRoot}/AzureSaturday.KEDA.Pusher/bin/Debug/netcoreapp3.0/AzureSaturday.KEDA.Pusher.dll",
      "args": [],
      "cwd": "${workspaceRoot}/AzureSaturday.KEDA.Pusher",
      "stopAtEntry": false,
      "console": "internalConsole"
    }
  ]
}
